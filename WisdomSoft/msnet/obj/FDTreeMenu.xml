				icon="imagelist:90"><![CDATA[
	<?php
	header('Content-Type: text/html;charset=UTF-8');
 	$a = array( 2.0, 2.0);
  $b = array( 1.0, 1.0);
  $z = array( 0.0, 0.0);
  print('<pre style="font-size:32px;">');
	printf('                 ★ 科学技術計算サブルーチンライブラリー（PHP）'."\n");
  printf('                2.1.2 複素数　加減算 （ＣＭＵＬ、ＣＤＩＶ）'."\n\n");
  
	$z = cmul($a, $b);
  $format = '    ( %5.2f + %5.2f*i ) * ( %5.2f + %5.2f*i ) = ( %5.2f + %5.2f*i )'."\n";
  printf( $format, $a[0], $a[1], $b[0], $b[1] , $z[0], $z[1]);
  
	$z = cdiv($a, $b);
  $format = '    ( %5.2f + %5.2f*i ) / ( %5.2f + %5.2f*i ) = ( %5.2f + %5.2f*i )'."\n";
  printf( $format, $a[0], $a[1], $b[0], $b[1] , $z[0], $z[1]);
	print('</pre>');

/**
 * cmul  複素数の積を求め結果を配列 [re,im]で返す
 *
 * {@source }
 * @access public
 * @param array [re,im]
 * @param array [re,im]
 * @return array 複素数の積を配列[re,im]で返す
 * @uses Sslib212()
 */
  function cmul($a, $b){
    $z = array();
  $z[1] = $a[0]*$b[1] + $b[0]*$a[1];
  $z[0] = $a[0]*$b[0] - $a[1]*$b[1];
  return $z;
  }
/**
 * cdiv  複素数の商を求め結果を配列 $z[re,im]で返す
 * {@source }
 * @access public
 * @param array [re,im]
 * @param array [re,im]
 * @return array 複素数の商を配列[re,im]で返す
 * @uses Sslib212()
 */
  function cdiv($a, $b){
    $z =array();
  
    if (cabs($b) == 0.0){
      $z[0] = 999.0;
      $z[1] = 999.0;
      return $z;
    }
    $z[1] = ($b[0]*$a[1]-$a[0]*$b[1])/($b[0]*$b[0]+$b[1]*$b[1]);
    $z[0] = ($a[0]*$b[0]+$a[1]*$b[1])/($b[0]*$b[0]+$b[1]*$b[1]);
    return $z;
  }
/**
 * cadd  複素数の和を求め結果を配列 [re,im]で返す
 * {@source }
 * @access public
 * @param array [re,im]
 * @param array [re,im]
 * @return array 複素数の和を配列[re,im]で返す
 * @see csub()
 */
  function cadd($a, $b){
    $z = array();
    $z[0] = $a[0] + $b[0];
    $z[1] = $a[1] + $b[1];
    return $z;
  }
/**
 * csub  複素数の差を求め結果を配列 [re,im]で返す
 * {@source }
 * @access public
 * @param array [re,im]
 * @param array [re,im]
 * @return array 複素数の差を配列[re,im]で返す
 * @see sslib211::sslib211()
 */
  function csub($a, $b){
    $z[0] = $a[0] - $b[0];
    $z[1] = $a[1] - $b[1];
    return $z;
  }
/**
 * cabs  複素数の絶対値を返す
 * {@source }
 * @access public
 * @param array
 * @return double 複素数の絶対値をdoubleで返す
 * @uses Sslib215()
 */
  function cabs($a){
    return sqrt($a[0]*$a[0]+$a[1]*$a[1]);
  }
//ＰＨＰの終わり
?>	

